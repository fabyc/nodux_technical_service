<data>
    <xpath
        expr="/form/notebook/page[@id='reports']" position="after">
        <page string="Technical Service" col="1" id="service">
            <newline/>
            <label name="logo"/>
            <field xfill="0" xexpand="1" name="logo" img_width="80" img_height="80" height="80" width="80" widget="image"/>
            <newline/>
            <separator name="description"/>
            <field name="description"/>
            <newline/>
        </page>
    </xpath>
</data>
